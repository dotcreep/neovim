return {
  "MarcHamamji/runner.nvim",
  dependencies = {
    "nvim-telescope/telescope.nvim",
    dependencies = { "nvim-lua/plenary.nvim" }
  },
  opts = {},
  keys = {
  },
  config = function(_, opts)
    local ok, run = require("runner")
    if not ok then vim.notify("runner plugin error") end
    run.setup(opts)
    vim.keymap.set("n", "<leader>rc", run.run, { desc = "Run this code file (runner)" })
  end
}
